{
  "name": "rollup-plugin-dev",
  "version": "2.0.0-beta.0",
  "description": "a development server for rollup",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/pearofducks/rollup-plugin-dev"
  },
  "scripts": {
    "dev": "cd dev && rollup -w -c",
    "dev:build": "cd dev && rollup -c",
    "build": "rollup -c",
    "version": "npm run build",
    "postversion": "npm publish && git push --follow-tags",
    "lint": "eslint --ignore-path .gitignore --ext .js .",
    "test": "ava --verbose"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "rollup",
    "development",
    "static",
    "proxy",
    "server"
  ],
  "author": "Dave Honneffer <pearofducks@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "colorette": "^1.2.2",
    "date-time": "^4.0.0",
    "fastify": "^3.18.0",
    "fastify-autoload": "^3.8.0",
    "fastify-http-proxy": "^6.0.0",
    "fastify-plugin": "^3.0.0",
    "fastify-static": "^4.2.2",
    "get-port": "^5.1.1",
    "joi": "^17.4.1",
    "ms": "^2.1.3"
  },
  "devDependencies": {
    "eslint": "^7.29.0",
    "rollup": "^2.52.3",
    "sinon": "^11.1.1",
    "supertest": "^6.0.1",
    "uvu": "^0.5.1"
  }
}
